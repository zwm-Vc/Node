<mvc:View controllerName="ui5.controller.mainView" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<content>
					<Button text="run" press="onPress"></Button>
					<Label text="start" id="label"></Label>
					<Button text="stop" press="onStop"></Button>
					<ProgressIndicator
						id="pi"
						class="sapUiSmallMarginBottom"
						percentValue="100"
						showValue="true"
						state="None" />
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>